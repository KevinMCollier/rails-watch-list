<div class="map" style="background-image: url('')"></div>
<div class="container">
  <h3 class="my-3">My Lists</h3>
  <div class="row justify-content-center">
    <% @lists.each do |list| %>
      <div class="col-md-4 col-12">
        <div class="card m-4">
          <h5><%= link_to list.name, list_path(list) %></h5>
        </div>
      </div>
    <% end %>
  </div>
</div>
